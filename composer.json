{
    "name": "shapecode/iban-bundle",
    "description": "iban generator",
    "keywords": [
        "shapecode",
        "iban",
        "bic",
        "generator",
        "symfony",
        "bundle"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "homepage": "https://github.com/shapecode/iban-bundle",
    "support": {
        "email": "support@nikita-loges.de",
        "issues": "https://github.com/shapecode/iban-bundle/issues",
        "source": "https://github.com/shapecode/iban-bundle/release",
        "wiki": "https://github.com/shapecode/iban-bundle/wiki"
    },
    "authors": [
        {
            "name": "Nikita Loges",
            "homepage": "http://nikita-loges.de",
            "email": "develop@nikita-loges.de"
        }
    ],
    "require": {
        "php": "~5.5|~7.0",

        "ext-soap": "*",

        "symfony/framework-bundle": "~2.3|~3.0|~4.0",
        "symfony/dependency-injection": "~2.3|~3.0|~4.0",
        "symfony/http-kernel": "~2.3|~3.0|~4.0",
        "symfony/config": "~2.3|~3.0|~4.0",

        "guzzlehttp/guzzle": "~6.3"
    },
    "autoload": {
        "psr-4": {
            "Shapecode\\Bundle\\IbanBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shapecode\\Bundle\\IbanBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
